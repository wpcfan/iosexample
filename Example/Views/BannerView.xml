<UIView clipsToBounds="true" outlet="layoutNode">
    <UIImageView
        bottom="parent.bottom"
        left="parent.left"
        right="parent.right"
        top="parent.top"
    />
    <UIView
        bottom="parent.bottom"
        center.x="parent.center.x"
        height="80%"
        width="80%">

        <FSPagerView.FSPagerView
            height="parent.height"
            outlet="pagerView"
            width="parent.width"
        />
        <FSPagerView.FSPageControl
            bottom="previous.bottom"
            height="32"
            left="previous.left"
            outlet="pagerControl"
            right="previous.right"
        />
    </UIView>
</UIView>
