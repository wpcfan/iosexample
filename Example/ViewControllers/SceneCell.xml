<UITableViewCell
    clipsToBounds="true"
    height="32"
    reuseIdentifier="SceneCell">

    <UIImageView
        height="32"
        imgUrl="{imageUrl}"
        left="parent.left + 16"
        width="32"
    />
    <UILabel
        left="previous.right + 10"
        text="{title}"
    />
</UITableViewCell>
